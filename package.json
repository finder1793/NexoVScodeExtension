{
    "name": "nexo-vscode-extension",
    "displayName": "Nexo VSCode Extension",
    "description": "VSCode support for Nexo configuration files",
    "version": "0.0.1",
    "license": "GPL-3.0",
    "engines": {
        "vscode": "^1.96.0"
    },
    "main": "./out/extension.js",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:yaml"
    ],
    "contributes": {
        "commands": [
            {
                "command": "nexo.generateResourcePack",
                "title": "Generate Resource Pack"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/finder1793/NexoVScodeExtension.git"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "@types/vscode": "^1.96.0",
        "typescript": "^5.7.2"
    }
}


